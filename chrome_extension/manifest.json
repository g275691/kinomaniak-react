{
    "manifest_version": 2,
    "name": "Kinogavnoru",
    "version": "1.0.0",
    
    "permissions": [
        "activeTab",
        "storage",
        "unlimitedStorage"    
    ],
    "content_scripts": [
        {    
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "src/scripts/setVolumeIndicator.js",
            "src/content.js"
            ]
        }     
    ],
      "icons":{"128": "src/icons/cat-ava.png"}
  }